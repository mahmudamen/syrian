<script language="javascript" type="text/javascript">
    function Gent() {
        $.ajax({
            url: '/Syrian/Casher/lastshift', cache: false, type: "POST", success: function (data) {
                $('#last').text(parseFloat(data).toFixed(0))
            }
        });
    }
    function shiftdate() {
        $.ajax({
            url: '/Syrian/Casher/shiftdate?id=' + parseInt($('#qdate').val()), cache: false, type: "POST", success: function (data) {
                $('#sft').val(data)
            }
        });
    }
    function ORDlst() {
        $.ajax({
            url: '/Syrian/Casher/ORDlst', cache: false, type: "POST", success: function (data) {
                $('#lastname').val(data)
            }
        });
    }
    function ORDaddress() {
        $.ajax({
            url: '/Syrian/Casher/ORDaddress', cache: false, type: "POST", success: function (data) {
                $('#adres').val(data)
            }
        });
    }
    function ORDtel() {
        $.ajax({
            url: '/Syrian/Casher/ORDtel', cache: false, type: "POST", success: function (data) {
                $('#tel').val(data)
            }
        });
    }
    function ORDmob() {
        $.ajax({
            url: '/Syrian/Casher/ORDmob', cache: false, type: "POST", success: function (data) {
                $('#mob').val(data)
            }
        });
    }
    function ORDlstnum() {
        $.ajax({
            url: '/Syrian/Casher/ORDlstnum', cache: false, type: "POST", success: function (data) {
                $('#orderfk').val(data)
            }
        });
    }
    function Ordpk() {
        $.ajax({
            url: '/Syrian/Casher/Ordpk', cache: false, type: "POST", success: function (data) {
                $('#orderpk').val(data)
            }
        });
    }
    function Totfuc() {
        $.ajax({
            url: '/Syrian/Casher/Totfuc?id=' + parseInt($('#last').text()), cache: false, type: "POST", success: function (data) {
                $('#Totfuc').text(parseFloat(data).toFixed(0))
            }
        });
    }
</script>
